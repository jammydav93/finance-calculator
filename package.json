{
  "name": "finance-forecaster",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.4",
    "@material-ui/icons": "^3.0.2",
    "chart.js": "^2.9.3",
    "classnames": "^2.2.6",
    "firebase": "^7.6.2",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-chartjs-2": "^2.8.0",
    "react-datepicker": "^2.11.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.2.6"
  },
  "devDependencies": {
    "@applitools/eyes-selenium": "^4.33.6",
    "@applitools/eyes-storybook": "^3.2.3",
    "@storybook/react": "^5.3.1",
    "body-parser": "^1.19.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "express": "^4.17.1",
    "redux-mock-store": "^1.5.4"
  },
  "scripts": {
    "start": "REACT_APP_VERSION=local REACT_APP_LAST_COMMIT_ID=local react-scripts start",
    "lint": "eslint --ext .js --ext .jsx .",
    "build": "REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') REACT_APP_LAST_COMMIT_ID=$(git rev-parse HEAD) react-scripts build",
    "appli-eyes": "APPLITOOLS_API_KEY=$APPLITOOLS_API_KEY npx eyes-storybook",
    "test": "npm run lint && react-scripts test --env=jsdom",
    "test-with-coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 2,
        "functions": 2,
        "lines": 2,
        "statements": 2
      }
    }
  }
}
